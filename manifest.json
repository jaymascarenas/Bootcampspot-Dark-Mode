{
  "short_name": "BCS Dark",
  "name": "Bootcampspot Dark Mode",
  "background": {
    "scripts": ["background.js"],
    "persistent": false
  },
  "icons": {
    "16": "images/bcs16.png",
    "32": "images/bcs32.png",
    "48": "images/bcs48.png",
    "128": "images/bcs128.png"
  },
  "permissions": [
    "activeTab",
    "tabs",
    "https://bootcampspot.com/*"
  ],
  "manifest_version": 2,
  "browser_action": {
    "default_title": "BCS Dark"
  },
  "version": "1.0"
}